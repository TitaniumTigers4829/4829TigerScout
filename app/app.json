{
  "expo": {
    "name": "TigerScout",
    "slug": "TigerScout",
    "privacy": "public",
    "description": "An easy, open source cloud-integrated scouting app made by 4829.",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E1E1E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/156bb3a7-5cf2-4e1d-b89f-e1e5a1f45f12"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.team4829.tigerscout",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow TigerScout to access your camera. TigerScout uses the camera exclusively to scan QR codes and allows you to connect to cloud storage buckets."
      }
    },
    "android": {
      "package": "com.team4829.tigerscout",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E1E1E"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "156bb3a7-5cf2-4e1d-b89f-e1e5a1f45f12"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow TigerScout to access your camera. TigerScout uses the camera exclusively to scan QR codes and allows you to connect to cloud storage buckets."
        }
      ]
    ]
  }
}
